<div id="map"></div>
<script type="text/javascript">
    function initMap(){
	var source = {lat: -33.861202, lng: 151.210810}; //Circular Quay Station
	var destination = {lat: -33.862984, lng: 151.208879}; //ThoughtWorks

	var map = new google.maps.Map(document.getElementById('map'), {
	center: source,
	scrollwheel: false,
	zoom: 7
	});

	var latlng = [{lat: -33.861987, lng: 151.210135}, {lat: -33.861708, lng:151.208753},{lat: -33.862761, lng: 151.208514}]

	latlng.forEach(function(geoloc) {
		var marker = new google.maps.Marker({
   	 	position: {lat: geoloc['lat'], lng: geoloc['lng']},
    	animation: google.maps.Animation.DROP,
    	map: map
   	});
    
	});

	var directionsDisplay = new google.maps.DirectionsRenderer({
	map: map
	});

	// Set destination, origin and travel mode.
	var request = {
	destination: destination,
	origin: source,
	travelMode: google.maps.TravelMode.WALKING

	//Other modes: DRIVING, CYCLING
	};

	// Pass the directions request to the directions service.
	var directionsService = new google.maps.DirectionsService();
	directionsService.route(request, function(response, status) {
	if (status === google.maps.DirectionsStatus.OK) {
	  // Display the route on the map.
	  directionsDisplay.setDirections(response);
	}
	});
}
 </script>

 <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAKZ3VotM3P69Zeu0GuZBqbGWGnVhiVIuc&callback=initMap"></script>